{
    "name": "job-task-3",
    "version": "1.0.0",
    "description": "Creating a Job Task without Sveltekit",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node backend/server",
        "server": "nodemon backend/server",
        "client": "npm run dev --prefix frontend",
        "client-phone": "npm run dev --prefix frontend-phone",
        "dev": "concurrently \"npm run server\" \"npm run client\" ",
        "dev-phone": "concurrently \"npm run server\" \"npm run client-phone\" ",
        "dev-all": "concurrently \"npm run server\" \"npm run client\" \"npm run client-phone\" "
    },
    "keywords": [
        "Svelte",
        "Nodejs",
        "MySql",
        "Express",
        "Sequelize"
    ],
    "author": "uf-code",
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^7.0.0",
        "nodemon": "^2.0.15"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.2",
        "framework7": "^6.3.16",
        "framework7-icons": "^5.0.5",
        "framework7-svelte": "^6.3.16",
        "mysql2": "^2.3.3",
        "sequelize": "^6.15.0"
    }
}
